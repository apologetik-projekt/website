{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public -b $(cat .env)",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "generate:css": "npx tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@cassiozen/usestatemachine": "1.0.1",
    "@remix-run/cloudflare": "1.3.5",
    "@remix-run/cloudflare-pages": "1.3.5",
    "@remix-run/react": "1.3.5",
    "clsx": "1.1.1",
    "framer-motion": "6.3.0",
    "ky": "0.33.3",
    "plausible-tracker": "0.3.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-lite-youtube-embed": "2.2.1-a"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.4.0",
    "@remix-run/dev": "1.3.5",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/typography": "0.5.2",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "10.4.4",
    "concurrently": "7.0.0",
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.12",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.3",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
